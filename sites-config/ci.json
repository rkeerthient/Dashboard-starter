{
  "artifactStructure": {
    "assets": [
      {
        "root": "dist",
        "pattern": "assets/**/*"
      },
      {
        "root": "dist",
        "pattern": "*"
      }
    ],
    "features": "sites-config/features.json",
    "plugins": [
      {
        "pluginName": "PagesGenerator",
        "sourceFiles": [
          {
            "root": "dist/plugin",
            "pattern": "*{.ts,.json}"
          },
          {
            "root": "dist",
            "pattern": "assets/{server,static,renderer,render}/**/*{.js,.css}"
          }
        ],
        "event": "ON_PAGE_GENERATE",
        "functionName": "PagesGenerator"
      },
      {
        "pluginName": "getFields-51782",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/fields/{{fieldId}}/getFields",
        "sourceFiles": [
          {
            "root": "dist/functions/getFields-51782",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "getFieldTypes-19820",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/fields/{{fieldId}}/getFieldTypes",
        "sourceFiles": [
          {
            "root": "dist/functions/getFieldTypes-19820",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "putFields-00828",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/fields/{{entityId}}/putFields",
        "sourceFiles": [
          {
            "root": "dist/functions/putFields-00828",
            "pattern": "*{.js,.ts}"
          }
        ]
      }
    ]
  },
  "dependencies": {
    "installDepsCmd": "npm install",
    "requiredFiles": [
      "package.json",
      "package-lock.json",
      ".npmrc"
    ]
  },
  "buildArtifacts": {
    "buildCmd": "npm run build:local"
  },
  "livePreview": {
    "serveSetupCmd": ":"
  }
}