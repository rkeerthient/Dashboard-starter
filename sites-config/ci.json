{
  "artifactStructure": {
    "assets": [
      {
        "root": "dist",
        "pattern": "assets/**/*"
      },
      {
        "root": "dist",
        "pattern": "*"
      }
    ],
    "features": "sites-config/features.json",
    "plugins": [
      {
        "pluginName": "PagesGenerator",
        "sourceFiles": [
          {
            "root": "dist/plugin",
            "pattern": "*{.ts,.json}"
          },
          {
            "root": "dist",
            "pattern": "assets/{server,static,renderer,render}/**/*{.js,.css}"
          }
        ],
        "event": "ON_PAGE_GENERATE",
        "functionName": "PagesGenerator"
      },
      {
        "pluginName": "id-02813",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/putFields/{{id}}",
        "sourceFiles": [
          {
            "root": "dist/functions/id-02813",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "id-75958",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/getFields/{{id}}",
        "sourceFiles": [
          {
            "root": "dist/functions/id-75958",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "id-61488",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/getFieldTypes/{{id}}",
        "sourceFiles": [
          {
            "root": "dist/functions/id-61488",
            "pattern": "*{.js,.ts}"
          }
        ]
      }
    ]
  },
  "dependencies": {
    "installDepsCmd": "npm install",
    "requiredFiles": [
      "package.json",
      "package-lock.json",
      ".npmrc"
    ]
  },
  "buildArtifacts": {
    "buildCmd": "npm run build:local"
  },
  "livePreview": {
    "serveSetupCmd": ":"
  }
}